plot_species <- function(experiment, reg_or_exper, units){
species_plot <- ggplot(emibc_experiment, aes(x = year, y = value, color = model)) +
labs(title=paste0('surface flux \n of BC - ', reg_or_exper), y=units, x="Year") +
theme_bw() +
theme(plot.title = element_text(hjust = 0.5, size = title_font),
axis.text = element_text(size = axis_font),
axis.title = element_text(size = axis_title_font)) +
scale_y_continuous(labels = scales::scientific_format(digits = 2)) +
scale_colour_manual(values = model_colors) +
geom_line()
return(species_plot)
}
